$ORIGIN nas.home.
$TTL 300
@	SOA	dns1.nas.home.	hostmaster.nas.home. (
		2019092215 ; serial
		21600      ; refresh after 6 hours
		3600       ; retry after 1 hour
		604800     ; expire after 1 week
		86400 )    ; minimum TTL of 1 day
;
;
	NS	dns1.nas.home.
dns1	A	192.168.0.102
;
;
; This sample zone file illustrates sharing the same IP addresses for multiple services:
;
www	A	192.168.0.102

media	CNAME	www.nas.home.
registry	CNAME	www.nas.home.
music	CNAME	www.nas.home.
adguard	CNAME	www.nas.home.
icecast	CNAME	www.nas.home.
;
