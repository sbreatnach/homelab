[Unit]
Description=A(MPD) Web UI
After=mpd.service

[Service]
WorkingDirectory={{ media.directories.ampd }}
ExecStart={{ media.java_home }}/bin/java -jar -Dserver.port=6660 -Dmpd.music.directory={{ media.directories.music }} {{ media.directories.ampd }}/ampd.jar
User=mpd
Restart=always

[Install]
WantedBy=multi-user.target