[Unit]
Description=Matrix-Facebook bridge
PartOf=matrix-synapse.target

[Service]
WorkingDirectory=/var/lib/matrix-synapse
ExecStart=/opt/venvs/matrix-synapse/bin/python -m mautrix_facebook -c /var/lib/matrix-synapse/mautrix-facebook.yaml -r /var/lib/matrix-synapse/mautrix-facebook.registration
ExecStart=/usr/local/src/mautrix-facebook/bin/python -m mautrix_facebook
User=matrix-synapse

[Install]
WantedBy=matrix-synapse.target